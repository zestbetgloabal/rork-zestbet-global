{
  "version": "1.0",
  "functions": {
    "api": {
      "runtime": "nodejs18.x",
      "handler": "index.handler",
      "environment": {
        "NODE_ENV": "production"
      }
    }
  },
  "rewrites": [
    {
      "source": "/api/<*>",
      "destination": "/api/index.js",
      "status": 200
    },
    {
      "source": "/trpc/<*>",
      "destination": "/api/index.js",
      "status": 200
    }
  ]
}